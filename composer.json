{
    "name": "imclarky/tesco-api",
    "type": "library",
    "description": "A PHP Client for the Tesco Supermarket API",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "vlucas/phpdotenv": "^2.5"
    },
    "autoload": {
        "classmap": [
            "src/TescoApi"
        ],
        "psr-4": {
            "ImClarky\\TescoApi\\": "src/TescoApi"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/unit"
        ],
        "psr-4": {
            "ImClarky\\TescoApi\\Tests\\": "tests/unit"
        }
    }
}
